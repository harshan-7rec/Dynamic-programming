#include <stdio.h>

int max(int a, int b) {
    if (a > b)
        return a;
    else
        return b;
}

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    int dp[n];
    int i, j;

    for (i = 0; i < n; i++)
        dp[i] = 1;  

    int maxLen = 1;

    for (i = 1; i < n; i++) {
        for (j = 0; j < i; j++) {
            if (arr[i] >= arr[j]) {
                int temp = dp[j] + 1;
                if (temp > dp[i])
                    dp[i] = temp;
            }
        }
        if (dp[i] > maxLen)
            maxLen = dp[i];
    }

    printf("%d\n", maxLen);
    return 0;
}
